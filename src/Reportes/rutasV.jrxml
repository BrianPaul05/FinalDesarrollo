	<queryString>
		<![CDATA[select rutas.cod_ruta, c.nom_ofi  as origen, c1.nom_ofi as destino,  rutas.precio, rutas.descripcion
from  oficinas, frecuencias, rutas
 join oficinas c on (rutas.cod_ofi_ori =c.cod_ofi )
 join oficinas c1 on (rutas.cod_ofi_des =c1.cod_ofi )
where rutas.cod_ofi_ori = oficinas.cod_ofi
and rutas.cod_ofi_des = oficinas.cod_ofi
and rutas.cod_ruta = frecuencias.cod_ruta_per
and rutas.estado = 'S']]>
